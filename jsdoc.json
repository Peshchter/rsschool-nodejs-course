{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "include": ["src"],
        "excludePattern": "(node_modules/|docs|jsdoc)"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "default": {
            "outputSourceFiles": true
        }
    },
    "opts":{
        "recurse" : true,
        "destination" : "./jsdoc/"
    }
}
